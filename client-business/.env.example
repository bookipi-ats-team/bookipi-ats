# Bookipi ATS - Environment Variables
# Copy this file to .env and update with your values

# API Configuration
# Set to false when connecting to real backend
VITE_USE_MOCK=true

# Backend API URL
# Update this when backend is ready
VITE_API_BASE_URL=http://localhost:3000/api/v1

# Authentication Token
# Will be provided by your auth flow
VITE_AUTH_TOKEN=
